{
  "name": "elastic-builder",
  "version": "0.4.4",
  "description": "A JavaScript implementation of the elasticsearch Query DSL",
  "main": "lib/index.js",
  "files": [
    "browser/",
    "lib/",
    "src/",
    "repl.js",
    "index.d.ts"
  ],
  "types": "./index.d.ts",
  "scripts": {
    "build:babel": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "build:umd": "webpack lib browser/elastic-builder.min.js",
    "build:docs": "documentation build src/index.js --github -o docs -f html -c ./docs/documentation.yml --name elastic-builder",
    "build": "npm run build:babel && npm run build:umd && npm run build:docs",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "precompile-tests": "cross-env NODE_ENV=test webpack --config webpack.config.test.js",
    "exec-tests": "cross-env NODE_ENV=test nyc --cache ava _build --concurrency 3",
    "test": "npm run precompile-tests && npm run exec-tests",
    "check": "npm run lint && npm test",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion-deprecated": "npm run check && npm run build && npm run git-commit",
    "postversion-deprecated": "npm run git-push",
    "git-commit": "git add docs browser && git commit --no-verify -m \"Commit built files\"",
    "git-push": "cross-env-shell \"git push origin master && git push origin v$npm_package_version\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudo-suhas/elastic-builder.git"
  },
  "keywords": [
    "elasticsearch",
    "search",
    "elasticjs",
    "elastic search",
    "elastic-builder",
    "ejs"
  ],
  "author": "Suhas Karanth <sudo.suhas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudo-suhas/elastic-builder/issues"
  },
  "homepage": "https://elastic-builder.js.org",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "test/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": {
    "colors": true,
    "run": [
      "lint-staged"
    ]
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.18.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "glob": "^7.1.1",
    "lint-staged": "^3.6.1",
    "lodash": "^4.17.4",
    "nyc": "^11.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.4.4",
    "sinon": "^2.3.4",
    "webpack": "^2.5.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "lodash.has": "^4.5.2",
    "lodash.hasin": "^4.5.2",
    "lodash.head": "^4.0.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isnil": "^4.0.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.omit": "^4.5.0"
  }
}
